{
    "name": "NbConverter",
    "displayName": "Jupyter Notebook Converter",
    "description": "Converts sectioned python files to Jupyter Notebooks",
    "version": "0.0.1",
    "publisher": "yigitozgumus",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.convertToNotebook"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.convertToNotebook",
                "title": "Convert file to Jupyter Notebook"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "python-shell": "^0.5.0"
    }
}
